<bean id="te" class="beans.Test">
   <property name="fruits">
     <util:list list-class="java.util.Vector">
       <value>Mango</value>
       <value>Apple</value>
       <value>Orange</value>
     </util:list>
   </property>
   
   <property name="cricketers">
     <util:set set-class="java.util.TreeSet">
        <value>Sachin</value>
        <value>Dhoni</value>
        <value>Rohit</value>
     </util:set>
   </property>

  <property name="StCap">
     <util:map map-class="java.map.Hashtable">
       <entry key="Maharashtra" value="Mumbai"/>
       <entry key="Gujrat" value="Gandhinagar"/>
       <entry key="Goa" value="Panaji"/>
     </util:map>
  </property>

</bean>
</beans>